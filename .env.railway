# Railway Environment Variables Template
# Copy these to your Railway project settings

# Core API Keys (REQUIRED)
OPENAI_API_KEY=your_openai_api_key_here
BROWSER_SERVICE_API_KEY=your_browser_service_api_key_here

# Optional API Keys
ANTHROPIC_API_KEY=
GOOGLE_API_KEY=
AZURE_OPENAI_API_KEY=
AZURE_OPENAI_ENDPOINT=
AZURE_OPENAI_API_VERSION=2025-01-01-preview
MISTRAL_API_KEY=
DEEPSEEK_API_KEY=
MOONSHOT_API_KEY=
ALIBABA_API_KEY=
UNBOUND_API_KEY=

# API Endpoints
OPENAI_ENDPOINT=https://api.openai.com/v1
ANTHROPIC_ENDPOINT=https://api.anthropic.com
MISTRAL_ENDPOINT=https://api.mistral.ai/v1
DEEPSEEK_ENDPOINT=https://api.deepseek.com
MOONSHOT_ENDPOINT=https://api.moonshot.cn/v1
ALIBABA_ENDPOINT=https://dashscope.aliyuncs.com/compatible-mode/v1
UNBOUND_ENDPOINT=https://api.getunbound.ai
OLLAMA_ENDPOINT=http://localhost:11434

# Browser Configuration
CHROME_PERSISTENT_SESSION=true
CHROME_DEBUGGING_PORT=9222
CHROME_DEBUGGING_HOST=
CHROME_CDP=http://127.0.0.1:9222
CHROME_USER_DATA=
CHROME_PATH=

# Display and VNC
RESOLUTION=1920x1080x24
RESOLUTION_WIDTH=1920
RESOLUTION_HEIGHT=1080
VNC_PASSWORD=vncpassword

# Application Settings
BROWSER_USE_LOGGING_LEVEL=info
ANONYMIZED_TELEMETRY=false

# Railway automatically sets PORT - don't override it
# PORT will be set by Railway
